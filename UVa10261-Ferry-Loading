	#include <bits/stdc++.h> 
	using namespace std;
	bool debug = false;
	int test, len, temp, n = 0;
	int cars[201], dp[10005][10005]; // remaining space on port and starboard
	int trace[101];

	int main() {
	    ifstream cin("ferry-loading.inp");
	    ios_base::sync_with_stdio(false);
	    cin.tie(NULL);
	    cin >> test;
	    while (test--) {
	    	n = 0; cars[n] = 0;
	    	cin >> len; 
	    	for (int i = 0; i <= len; i++) fill(dp[i], dp[i]+len, 0);
		    while (cin >> temp && temp != 0) {
				cars[++n] = temp / 100;
		    }
		    fill(trace, trace+n+1, -2);
		    dp[cars[1]][0] = 1; dp[0][cars[1]] = 1; trace[1]=1;
		    int maxCar = 1;
		    for (int car = 2; car <= n; car++) {
		    	for (int i = 0; i <= len; i++) {
		    		for (int j = 0; j <= len; j++) {
		    			if (i >= cars[car] && dp[i-cars[car]][j] == car-1) {
		    				if (debug) cout << i << ", " << j << ": " << car << ", trace: " << 0 << "\n";
		    				dp[i][j] = car;
		    				trace[car] = 0;
		    			}
		    			if (j >= cars[car] && dp[i][j-cars[car]] == car-1) {
		    				if (debug) cout << i << ", " << j << ": " << car << ", trace: " << 1 << "\n";
		    				dp[i][j] = car;
		    				trace[car] = 1;
		    			}
		    		}
		    	}
		    	if (trace[car] != -2) maxCar++;
		    }
		    if (debug) {
		    	for (int i = 0; i <= len; i++) {
		    			for (int j = 0; j <= len; j++) {
		    				cout << dp[i][j] << " ";
		    			}
		    		cout << "\n";
		    	}
		    }
		    cout << maxCar << "\n";
		    for (int i = 1; i <= maxCar; i++) {
		    	if (trace[i] == 0) cout << "starboard" << "\n";
		    	else cout << "port" << "\n";
		    }
		}
	    return 0;
	}	

